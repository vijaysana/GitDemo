<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="products">
  <test name="webinars">
    <classes>
      <class name="testvijay.testing3"/>
      <class name="testvijay.testng1"/>
      <class name="testvijay.testng2"/>
    </classes>
  </test>
  
  <test name="seminars">
  <classes>
    <class name="testvijay.testing4">
     <methods>
      <include name="virtualseminars"/>
     </methods>
    </class>
  </classes>
  </test>
  
  <test name="standards">
  <classes>
    <class name="testvijay.testing5">
      <methods>
        <exclude name="standards.*"></exclude>
      </methods>
    </class>
  </classes>
  </test>
</suite>
